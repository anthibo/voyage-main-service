   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/geojson": "^7946.0.8",
      "@types/jest": "^27.4.1",
      "@types/joi": "^17.2.3",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/morgan": "^1.9.3",
      "@types/multer": "^1.4.7",
      "@types/node": "^8.0.29",
      "aws-sdk": "^2.1127.0",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "cloudinary": "^1.30.0",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "joi": "^17.6.0",
      "jsonwebtoken": "^8.5.1",
      "moment": "^2.29.3",
      "morgan": "^1.10.0",
      "multer": "^1.4.4",
      "pg": "^8.7.3",
      "reflect-metadata": "^0.1.10",
      "ts-node": "3.3.0",
      "tslog": "^3.3.3",
      "typeorm": "0.2.41",
      "typescript": "3.3.3333",
      "uuid": "^8.3.2"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "start:dev": "nodemon src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migrate": "yarn typeorm",
      "migration:generate": "yarn run typeorm migration:generate --pretty --name",
      "migration:run": "yarn run typeorm migration:run",
      "migration:create": "yarn run typeorm migration:create --name",
      "migration:cli": "yarn run typeorm",
      "migration:revert": "yarn run typeorm migration:revert",
      "test": "jest"
   }
}
